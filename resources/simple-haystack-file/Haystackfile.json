{
  "variables": {
      "mysql_username": "[mysql-username]",
      "mysql_password": "vault[mysql-password]"
    }
  ,
  "services": {
    "laravel": {
      "package": "haystack/laravel",
      "src": "/html/http/www",
      "http_port": 8001,
      "https_port": 8002,
      "depends_on": "mysql"
    },
    "mysql": {
      "package": "haystack/mysql",
      "db_name": "laravel",
      "db_user": "$mysql_username",
      "db_password": "$mysql_password",
      "port": 8003
    }
  }
}














