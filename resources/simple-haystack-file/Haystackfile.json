{
  "variables": {
      "mysql_username": "[mysql-username]",
      "mysql_password": "vault[mysql-password]"
    }
  ,
  "services": {
    "php7.1": {
      "type": "docker.container",
      "image": "php:7.1-apache",
      "src": "/html/http/www",
      "ports": [
        {
          "container": "80",
          "host": "80"
        }
      ]
    },
    "mysql": {
      "type": "docker.container",
      "image": "mysql/mysql-server",
      "db": {
        "name": "laravel",
        "username": "mysql_username",
        "pw": "mysql_password"
      },
      "ports": [
        {
          "container": "3306",
          "host": "3306"
        }
      ]
    }
  }
}














